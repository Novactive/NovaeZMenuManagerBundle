{
  "extends": [
    "stylelint-config-standard"
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    at-rule-no-unknown: null,
    scss/at-rule-no-unknown: true,
    "no-descending-specificity": [
      null,
      {
        "severity": "warning"
      }
    ],
    "font-family-no-missing-generic-family-keyword": [
      true,
      {
        "severity": "warning"
      }
    ]
  }
}
